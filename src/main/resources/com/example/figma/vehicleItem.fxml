<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="vehiclePane" prefHeight="248.0" prefWidth="193.0" styleClass="vehicleItem" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.figma.vehicleItemController">
   <children>
      <VBox layoutX="-1.0" layoutY="7.0" prefHeight="238.0" prefWidth="195.0">
         <children>
            <HBox prefHeight="42.0" prefWidth="195.0">
               <children>
                  <Label fx:id="vehicleType" prefHeight="52.0" prefWidth="93.0" text="Car">
                     <HBox.margin>
                        <Insets left="40.0" right="10.0" />
                     </HBox.margin>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="vehicleID" layoutX="30.0" layoutY="10.0" prefHeight="52.0" prefWidth="93.0" text="#10">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="40.0" right="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <ImageView fx:id="vehicleImg" fitHeight="170.0" fitWidth="138.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets left="40.0" />
               </VBox.margin>
               <image>
                  <Image url="@images/car1.jpg" />
               </image>
            </ImageView>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" right="10.0" />
   </padding>
</AnchorPane>
