<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="82.0" prefWidth="1005.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.figma.videoItemController">
   <children>
      <Label fx:id="videoID" prefHeight="63.0" prefWidth="137.0" text="15">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="60.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="videoName" layoutX="10.0" layoutY="10.0" prefHeight="63.0" prefWidth="137.0" text="video.mp4">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="60.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="videoDate" layoutX="147.0" layoutY="10.0" prefHeight="63.0" prefWidth="137.0" text="15/12/2022">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="60.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="numberOfVehicles" layoutY="10.0" prefHeight="63.0" prefWidth="137.0" text="20">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="60.0" />
         </HBox.margin>
      </Label>
      <Button fx:id="exploreBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#viewDetectedVehicles" prefHeight="11.0" prefWidth="62.0" stylesheets="@css/style.css">
         <HBox.margin>
            <Insets left="50.0" />
         </HBox.margin>
         <graphic>
            <ImageView fx:id="searchBtn" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/search.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" top="20.0" />
   </padding>
</HBox>
